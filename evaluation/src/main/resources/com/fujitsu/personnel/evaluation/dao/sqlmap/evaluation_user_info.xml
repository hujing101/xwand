<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="User">
	<select id="getUserInfoByUserID" parameterType="com.fujitsu.evaluation.app.vo.UserVO" resultType="com.fujitsu.evaluation.app.vo.UserVO">
		select userID,userName from user where userID = #{userID} and password = #{password}
	</select>
</mapper> 