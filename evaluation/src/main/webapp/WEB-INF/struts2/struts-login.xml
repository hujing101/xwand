<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="login" extends="struts-default,json-default" namespace="/">
		<action name="login" class="loginAction">
			<result name="success">/jsp/layout/main.jsp</result>
			<result name="login">/jsp/login/index.jsp</result>
		</action>
		<action name="destory" class="loginAction" method="destory">
			<result type="json">
			</result>
		</action>
	</package>
</struts>
